{
    "agent": {
        "metrics_collection_interval": 60
    },
    "metrics": {
        "append_dimensions": {
            "AutoScalingGroupName": "${aws:AutoScalingGroupName}"
        },
        "metrics_collected": {
            "net": {
                "measurement": [
                    "bytes_sent", "bytes_recv", "packets_sent", "packets_recv"
                ],
                "resources": [
                  "*"
                ]
            }
        }
    }
}